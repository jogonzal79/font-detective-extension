{
  "manifest_version": 3,
  "name": "Font Detective",
  "version": "1.1",
  "description": "Identifica y analiza fuentes en páginas web con información detallada",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Font Detective - Analizar fuentes"
  },
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "32": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  }
}